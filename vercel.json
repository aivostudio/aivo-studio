{
  "functions": {
    "api/**/*.js": {
      "runtime": "nodejs18.x"
    }
  },
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/api/$1" },
    { "source": "/login", "destination": "/login.html" },
    { "source": "/checkout", "destination": "/checkout.html" },
    { "source": "/studio", "destination": "/studio.html" },
    { "source": "/studio/(.*)", "destination": "/studio.html" }
  ]
}
